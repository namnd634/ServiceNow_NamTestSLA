        <script><![CDATA[var DelayedAssignedToUpdate = Class.create();
DelayedAssignedToUpdate.prototype = {
    initialize: function() {},

    updateAssignedTo: function(incidentId, assignedTo) {
        var inc = new GlideRecord('incident');
        if (inc.get(incidentId)) {
            inc.u_incident_assigned_to = assignedTo;
            inc.update();
        }
    },

    type: 'DelayedAssignedToUpdate'
};
]]></script>
